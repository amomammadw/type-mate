{
    "name": "type-mate",
    "private": false,
    "version": "0.0.1",
    "type": "module",
    "main": "./dist/type-mate.umd.cjs",
    "module": "./dist/type-mate.js",
    "types": "./dist/index.d.ts",
    "license": "MIT",
    "keywords": [
        "helper",
        "typescript",
        "TypeMate",
        "lodash",
        "utility functions"
    ],
    "exports": {
