        "preview": "vite preview",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:preview": "vitepress preview docs"
    },
    "devDependencies": {
        "@types/node": "^20.11.0",
        "typescript": "^5.2.2",
        "vite": "^5.0.8",
        "vite-plugin-dts": "^3.7.0",
        "vitepress": "1.0.0-rc.36"
